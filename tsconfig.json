{
  "compilerOptions": {
    "outDir": "./out",
    "allowJs": true,
    "target": "es6",
    "module": "commonjs",
    "skipLibCheck": true,
    "inlineSourceMap": true,
    "strictNullChecks": false,
    "moduleResolution": "node"
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}